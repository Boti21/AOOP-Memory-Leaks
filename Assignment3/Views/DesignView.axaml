<UserControl x:Class="Assignment3.Views.DesignView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:vms="using:Assignment3.ViewModels"
             x:CompileBindings="False">
             
    <UserControl.DataContext>
        <vms:SelectionWindowViewModel/>
    </UserControl.DataContext>

    <UserControl.Resources>

    </UserControl.Resources>
    
    <Grid>
        <lvc:CartesianChart
            IsVisible="{Binding CartesianVisible}"
            Series="{Binding Series}"
            Title="{Binding Title}"
            XAxes="{Binding XAxes}"
            YAxes="{Binding YAxes}"
            LegendPosition="Right">
        </lvc:CartesianChart>
    </Grid>
</UserControl>