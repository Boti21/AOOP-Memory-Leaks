<UserControl x:Class="Assignment3.Views.DesignView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:vms="using:Assignment3.ViewModels"
             x:CompileBindings="False">
             
    <UserControl.DataContext>
        <vms:SelectionWindowViewModel/>
    </UserControl.DataContext>
    
    <lvc:CartesianChart
        Series="{Binding SelectedGraph.Series}"
        XAxes="{Binding SelectedGraph.XAxes}"
        YAxes="{Binding SelectedGraph.YAxes}"
        LegendPosition="Right">
    </lvc:CartesianChart>    
    
</UserControl>